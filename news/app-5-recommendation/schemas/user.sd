schema user {
    document user {

        field user_id type string {
            indexing: summary | attribute
            attribute: fast-search
        }

        field embedding type tensor<float>(d0[51]) {
            indexing: summary | attribute
        }

        field has_read_items type array<string> {
            indexing: summary | attribute
        }

    }

}
